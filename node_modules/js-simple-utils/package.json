{
	"name": "js-simple-utils",
	"title": "js-simple-utils",
	"version": "0.0.5",
	"description": "js-simple-utils",
	"type": "module",
	"keywords": [
		"js",
		"util"
	],
	"author": "duious",
	"license": "MIT",
	"homepage": "https://github.com/duious/js-simple-utils",
	"repository": {
		"type": "git",
		"url": "https://github.com/duious/js-simple-utils"
	},
	"engines": {
		"node": ">=10.x"
	},
	"publishConfig": {
		"registry": "https://registry.npm.com"
	},
	"files": [
		"cjs",
		"esm",
		"umd",
		"LICENSE",
		"README.md"
	],
	"main": "./esm/index.js",
	"unpkg": "./umd/index.js",
	"module": "./esm/index.js",
	"types": "./esm/index.d.ts",
	"scripts": {
		"clean": "rm -rf ./dist/* ./coverage/* ./docs/* ./umd ./esm ./cjs",
		"build:types": "tsc -b ./tsconfig.*.types.json",
		"build": "rm -rf ./dist/* ./umd ./esm ./cjs && npm run build:types && rollup --config ./build/rollup.config.ts && rollup --config ./build/rollup.umd.config.ts",
		"test": "npm run test-jest && npm run test-karma",
		"test-c": "jest --coverage",
		"test-karma": "node_modules/karma/bin/karma start karma.config.ts",
		"test-jest": "jest",
		"prepare": "husky install .husky",
		"eslint": "npx eslint --ext .ts --fix $npm_config_file",
		"doc": "rm -rf ./docs/* && mkdir -p ./docs && cp -R ./build/jsdoc/* ./docs/ && node_modules/.bin/jsdoc -c ./jsdoc.config.json && node --loader ts-node/esm ./build/jsdoc-outputFormat.ts",
		"release": "npm run build && npm publish"
	},
	"lint-staged": {
		"*.{js,cjs,ts}": [
			"eslint --color"
		]
	},
	"devDependencies": {
		"@babel/core": "^7.17.10",
		"@babel/plugin-transform-runtime": "^7.17.10",
		"@rollup/plugin-babel": "^5.3.1",
		"@rollup/plugin-commonjs": "^22.0.0",
		"@rollup/plugin-json": "^4.1.0",
		"@rollup/plugin-node-resolve": "^13.3.0",
		"@types/jest": "^27.5.0",
		"@types/jsdom": "^16.2.14",
		"@types/lodash": "^4.14.182",
		"@types/node": "^17.0.31",
		"@typescript-eslint/eslint-plugin": "^5.22.0",
		"@typescript-eslint/parser": "^5.22.0",
		"better-docs": "2.7.2",
		"eslint": "^8.15.0",
		"eslint-config-airbnb": "^19.0.4",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-jsx-a11y": "^6.5.1",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-react": "^7.30.0",
		"husky": "^7.0.4",
		"jasmine": "^4.1.0",
		"jasmine-browser-runner": "^1.0.0",
		"jasmine-core": "^4.1.1",
		"jest": "^28.1.0",
		"jest-environment-jsdom": "^28.1.0",
		"jsdoc": "3.6.10",
		"jsdoc-mermaid": "1.0.0",
		"jsdom": "^19.0.0",
		"karma": "^6.3.20",
		"karma-chrome-launcher": "^3.1.1",
		"karma-coverage": "^2.2.0",
		"karma-firefox-launcher": "^2.1.2",
		"karma-jasmine": "^5.0.1",
		"karma-rollup-preprocessor": "^7.0.8",
		"karma-safari-launcher": "^1.0.0",
		"karma-sauce-launcher": "^4.3.6",
		"lint-staged": "^12.4.1",
		"lodash": "^4.17.21",
		"moment": "^2.27.0",
		"prettier": "^2.6.2",
		"rollup": "^2.72.1",
		"rollup-plugin-babel": "^4.4.0",
		"rollup-plugin-istanbul": "^3.0.0",
		"rollup-plugin-node-resolve": "^5.2.0",
		"rollup-plugin-terser": "^7.0.2",
		"rollup-plugin-typescript2": "^0.31.2",
		"ts-jest": "^28.0.2",
		"ts-node": "^10.7.0",
		"typescript": "^4.6.4"
	},
	"dependencies": {}
}
